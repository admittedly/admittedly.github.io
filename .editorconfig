[*.js]
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.jade]
trim_trailing_whitespace = true
indent_style = tab
indent_size = 2

[*.scss]
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.json]
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = false